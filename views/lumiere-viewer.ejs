    /* —— Layout ——————————————————————————— */
    body {
      margin: 0;
      padding: 0;
      background: #000;
      display: flex;
      justify-content: center;
      align-items: center;
      min-height: 100vh;
      font-family: Arial, sans-serif;
    }

    .video-container {
      max-width: 100vw;
      max-height: 100vh;
